{
  "javascript.suggestionActions.enabled": false,
  "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "q": "do:exit"
  },

  // Turn on Log
  // :CocCommand workspace.showOutput
  // :CocCommand tsserver.openTsServerLog

  // "tsserver.trace.server": "verbose",
  // "tsserver.log": "verbose",
  // "tsserver.debugPort": 5859,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,

  // "solargraph.transport": "stdio",
  // "solargraph.trace.server": "verbose",
  // "solargraph.logLevel": "debug",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.pyenv/shims/black",
  "python.formatting.blackArgs": ["--diff", "--quiet", "--fast"],
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "~/.pyenv/shims/mypy",
  "python.linting.enabled": true,
  "snippets.ultisnips.enable": false,
  "coc.preferences.formatOnSaveFiletypes": ["python"],

  // when snippet activated and pumvisible, prefer complete completion.
  "suggest.preferCompleteThanJumpPlaceholder": true
}
